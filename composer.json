{
    "name": "syntech/syntechfcm",
    "description": "A Laravel package for FCM",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Syntech\\Syntechfcm\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Syntech\\Syntechfcm\\SyntechFcmServiceProvider"
            ]
        }
    },
    "require": {
        "php": ">=8.0"

    },
    "require-dev": {
        "orchestra/testbench": "^3.5"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    }
}
